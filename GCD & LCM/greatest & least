a = int (input ('enter your first number:'))
b = int (input ('enter your second number:'))

min = 0
if a > b:
    min = b
else:
    min = a

for i in range (min , 0 , -1 ):
    if a % i == 0 and b % i == 0:
        break
print ('GCD =',i)                       # Greatest Common Divisor
print ('LCM =' ,a * b / i)              # Least Common Multiple